    "./lib/hash256.js": "./lib/hash256-browser.js",
    "./lib/hash-drbg.js": "./lib/hash-drbg-browser.js",
    "./lib/hkdf.js": "./lib/hkdf-browser.js",
    "./lib/hmac-drbg.js": "./lib/hmac-drbg-browser.js",
    "./lib/internal/custom.js": "./lib/internal/custom-browser.js",
    "./lib/internal/pgpdf.js": "./lib/internal/pgpdf-browser.js",
    "./lib/keccak.js": "./lib/keccak-browser.js",
    "./lib/md2.js": "./lib/md2-browser.js",
